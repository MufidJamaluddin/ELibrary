{
  "name": "smartlibrarywebfrontend",
  "version": "0.1.0",
  "author": "Mufid Jamaluddin <mufidjamaluddin@outlook.com>",
  "private": true,
  "dependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node afterBuild.js",
    "test": "react-scripts test",
    "test:ci": "jest --coverage --runInBand --ci ",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.30",
    "@types/testing-library__jest-dom": "^5.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "ncp": "^2.0.0",
    "nyc": "^15.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/serviceWorker.js",
      "!**/index.js",
      "!*Transformer.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": -10
      }
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "reporter": [
      "cobertura"
    ],
    "report-dir": "../../test_results/UITests/"
  }
}
